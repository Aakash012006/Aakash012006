r=int(input("\nHow many yr your experienced : "))
s=int(input("\nEnter your Salary: "))
if(r>2):
    print("\nBonus + This month salary : ",(s+((4*s)/100)) )
elif(r>4):
    print("\nBonus + This month salary : ",(s+((7*s)/100)) )
elif(r>8):
    print("\nBonus + This month salary : ",(s+((10*s)/100)) )
else:
    print("\nBetter Luck Next Time")