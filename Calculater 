#include<stio.h>
int main() 
{
int num1, num2;
char op;
printf("Enter num1:") ;
scanf("%d", &num1) ;
printf ("Enter num2:") ;
scanf("%d", &num2) ;
printf("Enter a Operater:") ;
scanf("%c",&op) ;

switch (op) 
case'+':
printf ("Addition = %d", num1+num2) ;
break;
case'-':
printf ("Subtract= %d", num1-num2) ;
break;
case'*':
printf ("Multiplication = %d", num1*num2) ;
break;
case'/':
printf ("Division = %d", num1/num2) ;
default:
printf ("Invalid Operator ") ;
