a=0
b=0
c=0
for i in range(10):
    vote=int(input("Enter cnd's no. :"))
    if(vote==1):
        a+=1
    elif(vote==2):
        b+=1
    elif(c==1):
        c+=1
if(a!=0):
    if(a>b and a>b):
        print("A is Winner")
    else:
        print("A is Runner")
elif(b!=0):
    if(b>a and b>c):
        print("B is Winner")
    else:
        print("B is Runner")
elif(c!=0):
    if(c>a and c>b):
        print("C is Winner")
    else:
        print("C is Runner")